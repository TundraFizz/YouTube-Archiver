<!DOCTYPE html>
<html lang="en">
  <head>
    <title>NodeJS Example</title>
    <meta charset="utf-8">
    <meta name="description"  content="This is the index">
    <meta name="author"       content="Tundra Fizz">
    <link rel="shortcut icon" href="/img/icon.jpg">
    <link rel="stylesheet"    href="/css/index.css">
    <script src="js/jquery-3-3-1.js"></script>
    <script src="js/index.js"></script>
  </head>

  <body>
    <div>
      <p>Enter in a YouTube link</p>
    </div>

    <div>
      <input id="link" type="text" style="width: 400px;">
    </div>

    <div>
      <br>
      <input type="button" value="Submit" onclick="Archive();">
    </div>

    <br><br><br>

    <div>
      <table>
        <thead>
          <tr>
            <th>ID</th>
            <th>YouTube ID</th>
            <th>Title</th>
            <th>Duration</th>
            <th>Path Image</th>
            <th>Path Video</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i = 0; i < data.length; i++) { %>
            <tr>
              <td><%= data[i]["id"]         %></td>
              <td><%= data[i]["youtube_id"] %></td>
              <td><%= data[i]["title"]      %></td>
              <td><%= data[i]["duration"]   %></td>
              <td><%= data[i]["path_image"] %></td>
              <td><%= data[i]["path_video"] %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </body>
</html>
